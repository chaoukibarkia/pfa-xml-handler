{
  "name": "pfa-xml-handler",
  "version": "1.0.0",
  "description": "Large XML Public Figure Association Data Processor",
  "main": "bin/cli.js",
  "bin": {
    "pfa-xml-handler": "bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "pg": "^8.11.3",
    "winston": "^3.10.0",
    "xml-stream": "^0.4.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1"
  },
  "keywords": [
    "xml",
    "large-file",
    "data-processing",
    "postgresql",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off"
    }
  }
}